<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Spinner
        android:id="@+id/nation_select2"
        android:layout_width="100dp"
        android:layout_height="22dp"
        android:layout_marginLeft="124dp"
        android:layout_marginTop="323dp"
        android:entries="@array/nation"
        android:prompt="@string/str_prompt"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/nation_select3"
        android:layout_width="100dp"
        android:layout_height="22dp"
        android:layout_marginLeft="232dp"
        android:layout_marginTop="323dp"
        android:entries="@array/nation"
        android:prompt="@string/str_prompt"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include layout="@layout/toolbar_green" android:id="@+id/toolbar"
        />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="221dp"
        android:layout_marginTop="142dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginStart="221dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="142dp"
        android:layout_marginLeft="87dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="87dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="22dp"

        android:layout_marginLeft="68dp"
        android:layout_marginTop="272dp"
        android:gravity="center"
        android:text="주 사용 언어를 선택해주세요."
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/nation_select"
        android:layout_width="100dp"
        android:layout_height="22dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="323dp"
        android:entries="@array/nation"
        android:prompt="@string/str_prompt"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>