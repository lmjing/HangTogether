<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/toolbar_green" android:id="@+id/cs_toolbar"
        />

    <Button
        android:id="@+id/button_female"
        android:layout_width="123dp"
        android:layout_height="29dp"
        android:layout_marginLeft="210dp"
        android:layout_marginTop="460dp"
        android:background="#e1e1e1"
        android:ems="10"
        android:gravity="center"
        android:text="여자"
        android:textColor="#aaaaaa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_male"
        android:layout_width="122dp"
        android:layout_height="29dp"
        android:layout_marginLeft="88dp"
        android:layout_marginTop="460dp"
        android:background="#e1e1e1"
        android:ems="10"
        android:gravity="center"
        android:text="남자"
        android:textColor="#aaaaaa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="460dp"
        android:background="#e1e1e1"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text="성별"
        android:textColorHint="#aaaaaa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText_day"
        android:layout_width="63dp"
        android:layout_height="30dp"
        android:layout_marginLeft="270dp"
        android:layout_marginTop="418dp"
        android:background="#e1e1e1"
        android:ems="10"
        android:gravity="right|center_vertical"
        android:hint="일"
        android:inputType="textPersonName"
        android:textColorHint="#aaaaaa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText_month"
        android:layout_width="63dp"
        android:layout_height="30dp"
        android:layout_marginLeft="210dp"
        android:layout_marginTop="418dp"
        android:background="#e1e1e1"
        android:ems="10"
        android:gravity="right|center_vertical"

        android:hint="월"
        android:inputType="textPersonName"
        android:textColorHint="#aaaaaa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText_password2"
        android:layout_width="305dp"
        android:layout_height="30dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="377dp"
        android:background="#e1e1e1"
        android:ems="10"
        android:hint="비밀번호 확인"
        android:inputType="textPassword"
        android:paddingLeft="5dp"
        android:textColorHint="#aaaaaa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="418dp"
        android:background="#e1e1e1"
        android:ems="10"
        android:gravity="center"
        android:text="생일"
        android:textColorHint="#aaaaaa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText_year"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="418dp"
        android:background="#e1e1e1"
        android:ems="10"
        android:gravity="center"
        android:hint="년도(4자리)"
        android:inputType="textPersonName"
        android:textColorHint="#aaaaaa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText_password"
        android:layout_width="305dp"
        android:layout_height="30dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="335dp"
        android:background="#e1e1e1"
        android:ems="10"
        android:hint="비밀번호"
        android:inputType="textPassword"
        android:paddingLeft="5dp"
        android:textColorHint="#aaaaaa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText_nickname"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="294dp"
        android:background="#e1e1e1"
        android:ems="10"
        android:hint="닉네임"
        android:paddingLeft="5dp"
        android:inputType="textPersonName"
        android:textColorHint="#aaaaaa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_nickname"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginLeft="245dp"
        android:layout_marginTop="294dp"
        android:background="@drawable/rectangle_big"
        android:text="중복확인"
        android:textColor="#ffffff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="126dp"
        android:layout_height="123dp"
        app:srcCompat="@android:drawable/btn_dialog"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <EditText
        android:id="@+id/editText_email"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingLeft="5dp"
        android:hint="Email"
        android:textColorHint="#aaaaaa"
        android:background="#e1e1e1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="251dp"
        android:layout_marginLeft="28dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button_email"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="중복확인"
        android:textColor="#ffffff"
        android:background="@drawable/rectangle_big"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="251dp"
        android:layout_marginLeft="245dp"
        app:layout_constraintLeft_toLeftOf="parent" />

</android.support.constraint.ConstraintLayout>